{
  "name" : "Identify gene ontology signatures in gene expression data",
  "ver" : "1.0.0",
  "authors" : [ "pranjan77" ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "no-display"
  },
  
  "parameters" : [ {


            "id": "expression_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseFeatureValues.ExpressionMatrix"]
            }
        },


{
      "id": "input_condition_ids_samples",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [""],
      "field_type": "text"
},

   {
    "id" : "input_condition_ids",
    "optional" : true,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "textsubdata",
    "textsubdata_options" : {
      "subdata_selection": {
        "parameter_id" : "input_expression_matrix",
        "subdata_included" : ["data/col_ids"],
        "path_to_subdata": ["data","col_ids"]
      },
      "multiselection":true,
      "show_src_obj":false,
      "allow_custom":false
    }
  }
],

"parameter-groups": [
    {
      "id": "sample_n_conditions",
      "parameters": [
        "input_condition_ids_samples",
        "input_condition_ids"
      ],
      "optional": true,
      "allow_multiple": true,
      "with_border": false
    }
],

  "behavior" : {
    "service-mapping" : {
      "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property" : "workspace_name"
                },
                {
                    "input_parameter" : "expression_ref",
                    "target_type_transform": "resolved-ref",
                    "target_property" : "expression_ref"
                },
                {
                    "input_parameter": "sample_n_conditions",
                     "target_property": "sample_n_conditions"
                }
],




      "output_mapping" : [
        {
          "input_parameter": "input_expression_matrix",
          "target_property": "expressionMatrixID"
        },
        {
          "input_parameter": "sample_n_conditions",
          "target_property": "sample_n_conditions"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspaceID"
        }
      ]
    }
  }
}
